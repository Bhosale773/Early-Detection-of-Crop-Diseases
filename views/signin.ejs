<%- include ('./partials/header') %>

<title>Sign-In</title>
<link rel="stylesheet" href="../stylesheets/app.css">
<div class="form">
    <div class="border">
        <h3>Sign In</h3>
            <form class="form1" action="/sign-in" method="POST">
                <i class="fas fa-user i"></i>
                <input type="text" name="username" placeholder="Username*" required>
                <i class="fas fa-unlock-alt i"></i>
                <i class="eyeicon d-none far fa-eye i"></i>
                <input id="passinput" type="password" name="password" placeholder="Password*" required>
                <button type="submit" class="btn btn-primary">Sign In <i class="fas fa-sign-in-alt"></i></button>
            </form>
    </div>
    <div class="quote">
        <p>Don't have an account? <a href="/sign-up">Sign Up</a> </p>
    </div>
</div>

<%- include ('./partials/footer') %>